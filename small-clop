python experiment.py  \
 	--numClients 2 \
 	--numServers 1 \
 	--iNumber 4 \
    --coreAggrBW 500 \
    --aggrEdgeBW 250 \
    --edgeHostBW 125 \
    --expScenario base\
    --procTime 0.002 \
    --valueSizeModel pareto \
    --packetSize 0.00057 \
    --switchBufferSize 1 \
    --placementStrategy interleave \
	--numWorkload 1 \
	--workloadModel constant \
	--serverConcurrency 4 \
	--serviceTime 0.15 \
	--utilization 0.99 \
	--serviceTimeModel random.expovariate \
	--replicationFactor 10 \
	--backpressure \
	--rateInterval 20\
	--cubicC 0.000004\
	--cubicSmax 10\
	--cubicBeta 0.2\
	--hysterisisFactor 2\
	--selectionStrategy expDelay \
	--switchSelectionStrategy hopDelay\
	--shadowReadRatio 0.0 \
	--accessPattern uniform \
	--nwLatencyBase 0.0005 \
	--nwLatencyMu 0.0\
	--nwLatencySigma 0.00 \
	--expPrefix expDelay \
	--simulationDuration 10000000 \
	--seed 2 \
	--numRequests 1000\
	--logFolder logs\
	--demandSkew 0.0\
	--highDemandFraction 0.0\
	--slowServerFraction 0.0\
	--slowServerSlowness 0.0\
	--intervalParam 500\
	--timeVaryingDrift 5;


cd ..
cd plotting;
